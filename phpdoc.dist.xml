<?xml version="1.0" encoding="UTF-8" ?>
<phpdoc>
    <title>Siberian</title>
    <parser>
        <target>docs/phpdoc-cache</target>
        <encoding>utf8</encoding>
        <markers>
            <item>TODO</item>
            <item>FIXME</item>
        </markers>
        <extensions>
            <extension>php</extension>
            <extension>phtml</extension>
        </extensions>
        <visibility></visibility>
    </parser>
    <transformer>
        <target>docs/api</target>
        <external-class-documentation>
            <prefix>HTML_QuickForm2</prefix>
            <uri>http://pear.php.net/package/HTML_QuickForm2/docs/latest/HTML_QuickForm2/{CLASS}.html</uri>
        </external-class-documentation>
    </transformer>
    <transformations>
        <template name="responsive-twig"/>
    </transformations>
    <logging>
        <level>warn</level>
        <paths>
            <default>{APP_ROOT}/data/log/{DATE}.log</default>
            <errors>{APP_ROOT}/data/log/{DATE}.errors.log</errors>
        </paths>
    </logging>
    <transformations>
        <template name="responsive" />
    </transformations>
    <files>
        <directory>siberian/app/sae</directory>
        <directory>siberian/lib/Siberian</directory>
    </files>
</phpdoc>
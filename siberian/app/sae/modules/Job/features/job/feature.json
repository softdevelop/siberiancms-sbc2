{
    "name": "Job",
    "code": "job",
    "version": "4.16.5",
    "category": "social",
    "model": "Job_Model_Job",
    "desktop_uri": "job/application/",
    "routes": [
        {
            "state": "job-list",
            "controller": "JobListController",
            "url": "job/mobile_list/index/value_id/:value_id",
            "template": "l1/list.html",
            "cache": true,
            "root": true
        },
        {
            "state": "job-view",
            "controller": "JobViewController",
            "url": "job/mobile_view/index/value_id/:value_id/place_id/:place_id",
            "template": "l1/view.html",
            "cache": true
        },
        {
            "state": "company-view",
            "controller": "CompanyViewController",
            "url": "job/mobile_view/index/value_id/:value_id/company_id/:company_id",
            "template": "l1/view-company.html",
            "cache": true
        }
    ],
    "icons": [
        "icons/job1-flat.png",
        "icons/job1.png",
        "icons/job2-flat.png",
        "icons/job2.png"
    ],
    "files": [
        "js/controllers/job-list-controller.js",
        "js/controllers/job-view-controller.js",
        "js/controllers/company-view-controller.js",
        "js/factory/job.js",
        "scss/job.scss"
    ],
    "is_ajax": true,
    "compile": true,
    "use_account": false,
    "only_once": false
}

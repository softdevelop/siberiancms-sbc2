{
    "name": "WordPress (v2)",
    "code": "wordpress_v2",
    "lazyLoad": ["wordpress_v2"],
    "version": "0.0.1",
    "category": "integration",
    "model": "Wordpress2_Model_Wordpress",
    "desktop_uri": "wordpress2/application/",
    "routes": [
        {
            "root": true,
            "state": "wordpress2-list",
            "controller": "Wordpress2ListController",
            "url": "wordpress2/mobile_list/index/value_id/:value_id/query_id/:query_id",
            "template": "l1/list.html",
            "cache": false
        },
        {
            "state": "wordpress2-view",
            "controller": "Wordpress2ViewController",
            "url": "wordpress2/mobile_view/index/value_id/:value_id/post_id/:post_id",
            "template": "l1/view.html",
            "cache": false
        }
    ],
    "icons": [
        "icons/wordpress1.png",
        "icons/wordpress2.png"
    ],
    "files": [
        "js/controllers/wordpress_v2.js",
        "js/factory/wordpress_v2.js",
        "scss/wordpress2.scss"
    ],
    "is_ajax": true,
    "compile": true,
    "use_account": false,
    "only_once": false
}

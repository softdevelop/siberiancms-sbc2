{
    "name": "Folders (v2)",
    "code": "folder_v2",
    "version": "1.0.0",
    "category": "misc",
    "model": "Folder2_Model_Folder",
    "desktop_uri": "folder2/application/",
    "routes": [
        {
            "root": true,
            "state": "folder2-category-list",
            "controller": "Folder2ListController",
            "url": "folder2/mobile_list/index/value_id/:value_id/category_id/:category_id/layout_id/:layout_id",
            "template": "list.html",
            "layouts": {
                "1": "l1",
                "2": "l2",
                "3": "l3",
                "4": "l4",
                "5": "l5",
                "6": "l6"
            },
            "cache": false
        }
    ],
    "layouts": [
        1,
        2,
        3,
        4,
        5,
        6
    ],
    "icons": [
        "icons/folder1-flat.png",
        "icons/folder2-flat.png",
        "icons/folder3-flat.png"
    ],
    "files": [
        "js/controllers/folder_v2.js",
        "js/factory/folder_v2.js"
    ],
    "compile": true,
    "use_account": false,
    "only_once": false
}

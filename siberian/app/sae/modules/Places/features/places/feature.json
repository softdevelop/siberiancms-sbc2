{
    "name": "Places",
    "code": "places",
    "lazyLoad": {
        "core": ["cms"],
        "module": ["./features/places/places.bundle.min.js"]
    },
    "version": "4.16.5",
    "category": "misc",
    "model": "Places_Model_Place",
    "desktop_uri": "places/application/",
    "routes": [
        {
            "state": "places-categories",
            "controller": "PlacesCategoriesController",
            "url": "places/mobile_list/categories/value_id/:value_id",
            "template": "l1/categories.html",
            "cache": true,
            "root": true
        },
        {
            "state": "places-list",
            "controller": "PlacesListController",
            "url": "places/mobile_list/index/value_id/:value_id/category_id/:category_id",
            "template": "l1/places.html",
            "cache": true
        },
        {
            "state": "places-list-map",
            "controller": "PlacesMapController",
            "url": "places/mobile_list_map/index/value_id/:value_id",
            "template": "l1/maps.html",
            "cache": true
        },
        {
            "state": "places-view",
            "controller": "PlacesViewController",
            "url": "places/mobile_page_view/index/value_id/:value_id/page_id/:page_id",
            "template": "l1/view.html",
            "cache": true
        }
    ],
    "icons": [
        "icons/places-flat.png",
        "icons/places1-flat.png",
        "icons/places1-flat.png"
    ],
    "files": [
        "js/controllers/places-categories-controller.js",
        "js/controllers/places-list-controller.js",
        "js/controllers/places-map-controller.js",
        "js/controllers/places-view-controller.js",
        "js/factory/places.js",
        "scss/places.scss"
    ],
    "is_ajax": true,
    "compile": true,
    "use_account": false,
    "only_once": false
}

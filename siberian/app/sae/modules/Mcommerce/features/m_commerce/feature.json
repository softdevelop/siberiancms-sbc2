{
    "name": "Commerce",
    "code": "m_commerce",
    "version": "4.18.0",
    "category": "monetization",
    "model": "Mcommerce_Model_Mcommerce",
    "desktop_uri": "mcommerce/application/",
    "routes": [
        {
            "state": "mcommerce-category-list",
            "controller": "MCommerceListController",
            "url": "mcommerce/mobile_category/index/value_id/:value_id",
            "template": "l1/list.html",
            "cache": false,
            "root": true
        },
        {
            "state": "mcommerce-subcategory-list",
            "controller": "MCommerceListController",
            "url": "mcommerce/mobile_category/index/value_id/:value_id/category_id/:category_id",
            "template": "l1/list.html",
            "cache": false
        },
        {
            "state": "mcommerce-redirect",
            "controller": "MCommerceRedirectController",
            "url": "mcommerce/redirect/index/value_id/:value_id",
            "cache": false
        },
        {
            "state": "mcommerce-cart-view",
            "controller": "MCommerceCartViewController",
            "url": "mcommerce/mobile_cart/index/value_id/:value_id",
            "template": "l1/cart.html",
            "cache": false
        },
        {
            "state": "mcommerce-product-view",
            "controller": "MCommerceProductViewController",
            "url": "mcommerce/mobile_product/index/value_id/:value_id/product_id/:product_id",
            "template": "l1/product.html",
            "cache": false
        },
        {
            "state": "mcommerce-sales-confirmation",
            "controller": "MCommerceSalesConfirmationViewController",
            "url": "mcommerce/mobile_sales_confirmation/index/value_id/:value_id",
            "template": "l1/sales/confirmation.html",
            "cache": false
        },
        {
            "state": "mcommerce-sales-confirmation-cancel",
            "controller": "MCommerceSalesConfirmationCancelController",
            "url": "mcommerce/mobile_sales_confirmation/cancel/value_id/:value_id",
            "template": "l1/sales/confirmation.html",
            "cache": false
        },
        {
            "state": "mcommerce-sales-customer",
            "controller": "MCommerceSalesCustomerViewController",
            "url": "mcommerce/mobile_sales_customer/index/value_id/:value_id",
            "template": "l1/sales/customer.html",
            "cache": false
        },
        {
            "state": "mcommerce-sales-delivery",
            "controller": "MCommerceSalesDeliveryViewController",
            "url": "mcommerce/mobile_sales_delivery/index/value_id/:value_id",
            "template": "l1/sales/delivery.html",
            "cache": false
        },
        {
            "state": "mcommerce-sales-error",
            "controller": "MCommerceSalesErrorViewController",
            "url": "mcommerce/mobile_sales_error/index/value_id/:value_id",
            "template": "l1/sales/error.html",
            "cache": false
        },
        {
            "state": "mcommerce-sales-history",
            "controller": "MCommerceSalesHistoryViewController",
            "url": "mcommerce/mobile_sales_customer/history/value_id/:value_id",
            "template": "l1/sales/history.html",
            "cache": false
        },
        {
            "state": "mcommerce-sales-history-details",
            "controller": "MCommerceSalesHistoryDetailsController",
            "url": "mcommerce/mobile_sales_customer/history_details/value_id/:value_id/order_id/:order_id",
            "template": "l1/sales/history_details.html",
            "cache": false
        },
        {
            "state": "mcommerce-sales-payment",
            "controller": "MCommerceSalesPaymentViewController",
            "url": "mcommerce/mobile_sales_payment/index/value_id/:value_id",
            "template": "l1/sales/payment.html",
            "cache": false
        },
        {
            "state": "mcommerce-sales-store",
            "controller": "MCommerceSalesStoreChoiceController",
            "url": "mcommerce/mobile_sales_storechoice/index/value_id/:value_id",
            "template": "l1/sales/store.html",
            "cache": false
        },
        {
            "state": "mcommerce-sales-stripe",
            "controller": "MCommerceSalesStripeViewController",
            "url": "mcommerce/mobile_sales_stripe/index/value_id/:value_id",
            "template": "l1/sales/stripe.html",
            "cache": false
        },
        {
            "state": "mcommerce-sales-success",
            "controller": "MCommerceSalesSuccessViewController",
            "url": "mcommerce/mobile_sales_success/index/value_id/:value_id",
            "template": "l1/sales/success.html",
            "cache": false
        }
    ],
    "icons": [
        "icons/m_commerce1-flat.png",
        "icons/m_commerce2-flat.png",
        "icons/m_commerce3-flat.png"
    ],
    "files": [
        "js/controllers/cart.js",
        "js/controllers/category.js",
        "js/controllers/product.js",
        "js/controllers/sales/confirmation.js",
        "js/controllers/sales/customer.js",
        "js/controllers/sales/delivery.js",
        "js/controllers/sales/error.js",
        "js/controllers/sales/history.js",
        "js/controllers/sales/payment.js",
        "js/controllers/sales/store.js",
        "js/controllers/sales/stripe.js",
        "js/controllers/sales/success.js",
        "js/factory/cart.js",
        "js/factory/category.js",
        "js/factory/product.js",
        "js/factory/sales/customer.js",
        "js/factory/sales/delivery.js",
        "js/factory/sales/payment.js",
        "js/factory/sales/store.js",
        "js/factory/sales/stripe.js",
        "scss/mcommerce.scss"
    ],
    "is_ajax": true,
    "compile": true,
    "use_account": true,
    "only_once": true
}
